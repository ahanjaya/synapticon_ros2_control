<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

	<xacro:macro name="six_dof" params="parent prefix *origin">
		<joint name="${prefix}base_joint" type="fixed">
			<parent link="${parent}" />
			<child link="${prefix}motor_link1" />
			<origin xyz="0 -3.3 0" rpy="0 0 0" />
		</joint>

		<!-- Link 1 -->
		<link name="${prefix}motor_link1">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0" />
