			<parent link="${prefix}motor_link1" />
			<child link="${prefix}flywheel_link1" />
			<origin xyz="0 0 0" rpy="0 0 0" />
			<axis xyz="0 0 1" />
			<dynamics damping="0.7" />
			<limit effort="100" velocity="1.0" />
