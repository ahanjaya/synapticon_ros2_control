					<mesh filename="package://synapticon_ros2_control/meshes/motor.stl"
						scale="0.005 0.005 0.005" />
				</geometry>
			</visual>
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0" />
				<geometry>
					<mesh filename="package://synapticon_ros2_control/meshes/motor.stl"
						scale="0.005 0.005 0.005" />
				</geometry>
			</collision>
		</link>

